{
  "manifest_version": 2,

  "name": "KeepinTabs",
  "description": "This extension organizes a users browser tabs",
  "version": "1.0",
  "content_security_policy": "script-src 'self' http://127.0.0.1; object-src 'self'",
  "browser_action": {
    "default_icon": "keeping-tabsicon.png",
    // "default_popup": "Index.html",
    "default_title": "JUSTIN LIM IN DA BROWSER"
  },
  "permissions": [
    "activeTab",
    "tabs", 
    "http://localhost/*"
  ],
  "background": {
    "page": "background.html"
  }
}
